  "scripts": {
    "build": "cross-env NODE_ENV=production node build/build.js",
    "build-dev": "cross-env NODE_ENV=production STAGE=dev node build/build.js",
    "build-staging": "cross-env NODE_ENV=production STAGE=staging node build/build.js",
    "build-prod": "cross-env NODE_ENV=production STAGE=prod node build/build.js",
    "clean": "rm -rf dist",
    "dev": "cross-env NODE_ENV=development STAGE=dev node build/dev-server.js",
    "start": "cross-env NODE_ENV=development STAGE=dev node build/dev-server.js",
    "staging": "cross-env STAGE=staging node build/dev-server.js",
    "prod": "cross-env STAGE=prod node build/dev-server.js",
    "electron": "cross-env NODE_ELECTRON=true npm run build && electron electronIndex.js",
    "gh": "npm run build && gh-pages -d dist",
    "lint": "eslint --ext .js .vue client/*",
    "lint:fix": "eslint --fix --ext .js .vue electron.js client/* build/* config/*",
    "test": "echo lol",
    "deploy": "git subtree push --prefix dist heroku master",
    "aws-prod": "git subtree push --prefix dist aws master",
    "aws-dev": "git subtree push --prefix dist aws-dev master",
    "aws-staging": "git subtree push --prefix dist aws-staging master"
  },
